        </HBox>
    
    </children>
    <children>
        <VBox>
            <ListView fx:id="commandListView">
                <!-- Contenu de la liste Ã  remplir dynamiquement -->
            </ListView>
        </VBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
